<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Prediction - Predict Your DNA Mutation</title>
    <link
        href="https://cdn.jsdelivr.net/npm/remixicon@4.5.0/fonts/remixicon.css"
        rel="stylesheet"
    />
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=IBM+Plex+Mono:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;1,100;1,200;1,300;1,400;1,500;1,600;1,700&family=Inter:ital,opsz,wght@0,14..32,100..900;1,14..32,100..900&family=Oooh+Baby&family=Questrial&family=Splash&family=Wix+Madefor+Text:ital,wght@0,400..800;1,400..800&display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css2?family=IBM+Plex+Mono:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;1,100;1,200;1,300;1,400;1,500;1,600;1,700&family=Inter:ital,opsz,wght@0,14..32,100..900;1,14..32,100..900&family=Oooh+Baby&family=Questrial&family=Splash&family=Wix+Madefor+Text:ital,wght@0,400..800;1,400..800&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="css/prediction.css" />
  </head>
  <body style="background: #f6f6f6;">
    <form action="#">
      <div class="form-top-container-for-date">
        <div class="date-container">
          19
        </div>
        <div class="day-month-container">
          Tue,
          December
        </div>
        <div class="form-top-container-line"></div>
        <div class="form-home-container-button" onclick="window.location.href = '/'">
          Home
          <i class="ri-arrow-right-line"></i>
        </div>
      </div>
      <textarea placeholder="Need help decoding your DNA? |Just paste your sequence and hit predict to get started."></textarea>
      <button type="submit">
        Predict
      </button>
    </form>

    <div class="second-row">
      <% data.forEach(feature => { %>
          <div class="second-card-container">
          <p class="second-card-main-heading">
            <%= feature.title %> 
          </p>
          <h1 class="second-card-main-content">
            <%= feature.content %>  
          </h1>
          <div class="second-row-card-bottom-container">
            <div 
            <% if (feature.percentage>50) { %>
             style="color: green; background: #0080004f"
             <% } else { %>
              style="color: red; background: rgba(255, 0, 0, 0.2);"
            <% } %>

            class="second-row-card-bottom-right-container">
              <% if (feature.percentage > 50) { %>
                  <i class="ri-arrow-up-line"></i>
                <% } else { %>
                  <i class="ri-arrow-down-line"></i>
              <% } %>

              <% if (feature.percentage == 'High') { %>
                <%= feature.percentage %>
              <% } else { %>
                <%= feature.percentage %>%
              <% } %>
            </div>
            <p>
              <%= feature.para %>
            </p>
          </div>
        </div>
      <% }) %>
    </div>

    <div class="third-row">
      <div class="label-prediction-container">
        <p class="label-prediction-main-heading">
          Predicted Labels
        </p>
        <div class="label-prediction-main-prediction-container">
          <div class="label-prediction-main-prediction-top-container">
            <p>
              <%= Object.keys(label[0])[0] %>
            </p>
            <div class="label-prediction-main-prediction-cofidence-container">
              <%= label[0]['Pathogenic'] %>%
            </div>
          </div>
          <div class="label-prediction-main-line"></div>
        </div>
        <div class="label-prediction-main-bar-chart-container">
          <div class="label-prediction-main-bar-chart-left-container">
            <% label[1].forEach(labels => { %>
             <p><%= labels %></p>
            <% }) %>
          </div>
          <div class="label-prediction-main-bar-chart-right-container">
            <% label[2].forEach(labelConfi => { %>
            <div class="label-prediction-main-line-container">
              <div
              style="width: <%= labelConfi*0.9 %>%;"
              title="<%= labelConfi%>%"
              class="label-prediction-main-line-line"></div>
              <div class="label-prediction-main-line-percentange"> <%= labelConfi %> %</div>
            </div>
            <% }) %>
          </div>
        </div>
      </div>
