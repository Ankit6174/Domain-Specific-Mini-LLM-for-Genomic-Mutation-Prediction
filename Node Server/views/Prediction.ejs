    <div class="forth-row">
      <div class="sliding-window-gc-content-container">
        <p class="sliding-window-gc-content-main-heading">
          Sliding Window GC Content
        </p>
        <div class="sliding-window-gc-content-middle-container">
          <div class="sliding-window-gc-content-middle-left-container">
            <i class="ri-arrow-right-up-line"></i>
            70%
          </div>
          <p>Window size 30</p>
        </div>
        <div class="sliding-window-gc-content-bottom-container">
          <% slicingGC[1].forEach(content => { %>
           <div class="sliding-window-gc-content-bottom-bar-container">
            <p> <%= content %>%</p>
            <div style="height: <%= content*0.8 %>%;" class="sliding-window-gc-content-bottom-bar"></div>
          </div>
          <% }) %>
          
        </div>
      </div>

      <div class="at-gc-content-container">
        <div class="at-gc-content-top-container">
          <p class="at-gc-content-main-heading">GC/AT Content</p>
          <div class="at-gc-content-top-right-container">
            <% at_gc_content.forEach(contents => { %>
              <div class="at-gc-content-top-right-top-container">
                <div 
                style="background: <%= contents[2] %> ;"
                 class="at-gc-content-top-right-top-left"></div>
                <div class="at-gc-content-top-right-top-middle">
                  <%= contents[0] %>
                </div>
                <div
                <% if (contents[1] > 50) { %>
                 style="background: rgba(9, 255, 0, 0.3); color: green;"
                 <% } else { %>
                  style="background: rgba(255, 0, 0, 0.3); color: red;"
                <% } %>
                class="at-gc-content-top-right-top-right">
                  <% if (contents[1] > 50) { %>
                    <i class="ri-arrow-right-up-line"></i>
                    <% } else { %>
                     <i class="ri-arrow-left-down-line"></i>
                  <% } %>
                  <%= contents[1] %>%
                </div>
              </div>
            <% }) %>
          </div>
        </div>
        <div class="at-gc-content-bottom-container">
          <% for( let index = 0; index < at_gc_content[1][1]; index++ ) { %>
            <div style="background: <%= at_gc_content[1][2] %>;" class="gc-content"></div>
            <% } %>
            <% for( let index = 0; index < at_gc_content[0][1]; index++ ) { %>
              <div style="background: <%= at_gc_content[0][2] %>;" class="at-content"></div>
          <% } %>
        </div>
      </div>

      <div class="nucleotide-frequency-container">
        <p class="nucleotide-frequency-main-heading">Nucleotid Frequency</p>
        <div class="nucleotide-frequency-middle-container">
          <div class="nucleotide-frequency-middle-left-container">
            <i class="ri-arrow-right-up-line"></i>
            G
          </div>
          <p>Minimum Frequency</p>
        </div>
        <div class="nucleotide-frequency-bottom-container">
          <% nucleoFreq.forEach(freq => { %>
           <div class="nucleotide-frequency-bottom-bar-container">
            <p> <%= freq[0] %> </p>
            <div style="height: <%= freq[1] %>%;" class="nucleotide-frequency-bottom-bar"></div>
          </div>
          <% }) %>
        </div>
      </div>
    </div>

    <div class="fifth-row">
      <div class="fifth-container-prediction-container">
        <p class="fifth-container-headings">mRNA Sequence</p>
        <div class="fifth-container-second-row-container">
          <p>5’ PRIME</p>
          <p>3’ PRIME</p>
        </div>
        <div class="mrna-canvas">
          <div class="start-dna">
